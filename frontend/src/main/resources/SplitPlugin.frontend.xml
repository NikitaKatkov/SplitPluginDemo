<idea-plugin>
    <dependencies>
        <module name="intellij.platform.frontend"/>
        <module name="SplitPlugin.shared"/>

        <module name="intellij.libraries.skiko"/>
        <module name="intellij.libraries.compose.foundation.desktop"/>
        <module name="intellij.platform.jewel.foundation"/>
        <module name="intellij.platform.jewel.ui"/>
        <module name="intellij.platform.jewel.ideLafBridge"/>
    </dependencies>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="org.jetbrains.plugins.template.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>

        <toolWindow id="TestToolWindow"
                    icon="AllIcons.Actions.AddToDictionary"
                    factoryClass="com.example.splitplugin.frontend.FrontendTestToolwindowFactory"
                    secondary="false"
                    anchor="left"/>
    </extensions>
</idea-plugin>